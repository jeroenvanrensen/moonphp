{
    "name": "jeroenvanrensen/moonphp",
    "description": "An admin panel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van Rensen",
            "email": "jmjrensen@jcjcj.nl",
            "homepage": "https://www.jeroenvanrensen.nl/",
            "role": "Developer"
        }
    ],
    "require": {
        "livewire/livewire": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "JeroenvanRensen\\MoonPHP\\": "src",
            "JeroenvanRensen\\MoonPHP\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenvanRensen\\MoonPHP\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenvanRensen\\MoonPHP\\MoonServiceProvider"
            ]
        }
    }
}
